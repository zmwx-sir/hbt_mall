/* 轮播图 */
.index_swiper swiper {
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
/* 1Floor分类导航 */
.index_cate {
  margin-top: 10rpx;
  display: flex;
}
.index_cate navigator {
  flex: 1;
}
.index_cate image {
  width: 150rpx;
}
/* 2Floor楼层数据 */
.index_floor .floor_group .floor_title {
  padding: 10px 0px;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_product navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor_group .floor_product navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_group .floor_product navigator:nth-child(2),
.index_floor .floor_group .floor_product navigator:nth-child(3){
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_product navigator image {
  width: 100%;
  height: 100%;
}